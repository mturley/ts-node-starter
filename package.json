{
  "name": "ts-node-starter",
  "version": "1.0.0",
  "description": "A barebones starter repo for NodeJS projects in TypeScript with cold reload for dev mode.",
  "scripts": {
    "build": "npm run clean && npx tsc",
    "start": "node build/index.js",
    "build:start": "npm run build && npm run start",
    "start:dev": "nodemon",
    "clean": "rimraf ./build",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mturley/ts-node-starter.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mturley/ts-node-starter/issues"
  },
  "homepage": "https://github.com/mturley/ts-node-starter#readme",
  "dependencies": {
    "square": "^25.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
